{
    "name": "wiinkil",
    "private": false,
    "version": "0.1.0",
    "license": "MIT",
    "repository": "git@github.com:Mecuate/wiinkil.git",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:full": "node config/develop.cjs",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest --logHeapUsage --watch=false",
        "mk.ic": "node config/makeIcons.cjs",
        "mk.models": "node config/makeModels.cjs",
        "mk.img": "node config/makeImages.cjs",
        "mk.keys": "node config/makeTranslations.cjs",
        "mk": "node config/create.cjs",
        "typecheck": "tsc -p tsconfig.json",
        "format": "prettier --config .prettierrc.json src/**/*.{ts,tsx} --write",
        "prepare": "husky install"
    },
    "dependencies": {
        "monaco-editor": "^0.36.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@stitches/react": "^1.2.8",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/node": "^18.6.2",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "@types/react-router": "^5.1.18",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react": "^3.1.0",
        "@vitest/coverage-c8": "^0.24.5",
        "@vitest/ui": "latest",
        "jsdom": "latest",
        "husky": "8.0.3",
        "prettier": "^2.7.1",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "typescript": "^4.9.3",
        "vite": "latest",
        "vite-tsconfig-paths": "^3.5.0",
        "vitest": "latest"
    }
}
